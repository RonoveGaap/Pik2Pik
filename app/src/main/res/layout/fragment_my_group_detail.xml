<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.myGroupDetailFragment.MyGroupDetailFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.23"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.17"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.53"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_16"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_17"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.64"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_18"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_vertical_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.06"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_vertical_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_vertical_3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_vertical_5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.12"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/fragment_my_group_detail_guideline_vertical_6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.88"/>


    <ImageView
        android:id="@+id/my_group_detail_back_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_1"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_1"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_1"
        app:layout_constraintWidth_percent="0.15"
        app:layout_constraintDimensionRatio="1:0.7"
        android:src="@drawable/back_arrow"
        />

    <TextView
        android:id="@+id/my_group_detail_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_2"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="30sp"
        style="@style/TitleText"
        android:text=""
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_extra_bold"
        android:text="Users"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_3"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_2"
        app:layout_constraintRight_toRightOf="@id/fragment_my_group_detail_guideline_vertical_2"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_3" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/my_group_detail_member_recycler_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_2"
        app:layout_constraintRight_toRightOf="@id/fragment_my_group_detail_guideline_vertical_2"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_4"
        android:background="@drawable/border_radius_white_rectangle"
        app:layout_constraintWidth_percent="0.35"
        app:layout_constraintHeight_percent="0.3"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_5"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_6"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_3"
        app:layout_constraintRight_toRightOf="@id/fragment_my_group_detail_guideline_vertical_3"
        app:layout_constraintWidth_percent="0.35"
        android:background="@drawable/border_radius_black_rectangle"
        >

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragment_my_group_detail_guideline_7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.09"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragment_my_group_detail_guideline_8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.17"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragment_my_group_detail_guideline_9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragment_my_group_detail_guideline_vertical_4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_extra_bold"
            android:text="Data"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_7" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_8"
            app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_9"
            app:layout_constraintWidth_percent="0.8"
            android:background="@drawable/orange_gradient_button_background"
            >

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fragment_my_group_detail_guideline_10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.15"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fragment_my_group_detail_guideline_11"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.25"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fragment_my_group_detail_guideline_12"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.45"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fragment_my_group_detail_guideline_13"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.55"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fragment_my_group_detail_guideline_14"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.75"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fragment_my_group_detail_guideline_15"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.85"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold_italic"
                android:text="Date"
                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_10"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_10" />

            <TextView
                android:id="@+id/my_group_detail_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_11"
                app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_11"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:textSize="14sp"
                android:fontFamily="@font/montserrat_regular"
                android:text=""
                android:textColor="@color/black"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold_italic"
                android:text="Pictures"
                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_12"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_12" />

            <TextView
                android:id="@+id/my_group_detail_number_of_images"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_13"
                app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_13"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:textSize="14sp"
                android:fontFamily="@font/montserrat_regular"
                android:text=""
                android:textColor="@color/black"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold_italic"
                android:text="Video?"
                android:textColor="@color/black"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_14"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_14" />

            <TextView
                android:id="@+id/my_group_detail_is_video_generated"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_15"
                app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_15"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:textSize="14sp"
                android:fontFamily="@font/montserrat_regular"
                android:text=""
                android:textColor="@color/black"
                />



        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_extra_bold"
        android:text="Pictures"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_16"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_5"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_16" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/my_group_detail_image_recycler_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_5"
        app:layout_constraintRight_toRightOf="@id/fragment_my_group_detail_guideline_vertical_6"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_17"
        app:layout_constraintHeight_percent="0.15"
        android:paddingStart="8dp"
        android:paddingEnd="8dp"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:background="@drawable/border_radius_white_rectangle"
        />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/my_group_detail_view_video_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_18"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_18"
        app:layout_constraintLeft_toLeftOf="@id/fragment_my_group_detail_guideline_vertical_5"
        app:layout_constraintWidth_percent="0.35"
        app:layout_constraintHeight_percent="0.1"
        android:background="@drawable/white_full_border_ripple"
        android:visibility="visible">


        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:src="@drawable/my_group_view_video_icon_orange"
            app:layout_constraintHeight_percent="0.8"
            app:layout_constraintDimensionRatio="1"/>



    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/my_group_detail_delete_group_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/fragment_my_group_detail_guideline_18"
        app:layout_constraintBottom_toBottomOf="@id/fragment_my_group_detail_guideline_18"
        app:layout_constraintRight_toRightOf="@id/fragment_my_group_detail_guideline_vertical_6"
        app:layout_constraintWidth_percent="0.35"
        app:layout_constraintHeight_percent="0.1"
        android:background="@drawable/white_full_border_ripple"
        android:visibility="visible">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:src="@drawable/my_groups_delete"
            app:layout_constraintHeight_percent="0.7"
            app:layout_constraintDimensionRatio="1"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>